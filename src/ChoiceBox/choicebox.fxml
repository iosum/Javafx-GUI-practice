<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ChoiceBox.ChoiceboxController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Choice Box Exercise" />
            <ChoiceBox fx:id="choicebox" prefWidth="150.0" />

            <Button mnemonicParsing="false" onAction="#chooseButtonClicked" text="Choose" />
            <Label fx:id="choiceboxLabel" text="Label" />
         </children>
      </VBox>
   </children>
</AnchorPane>
